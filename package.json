{
  "name": "nx-angular-starter",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "yarn": "please-use-pnpm",
    "npm": "please-use-pnpm",
    "pnmp": ">= 15.4.3"
  },
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "check-packages": "npm-check",
    "update-packages": "npm-check -y",
    "git-add": "git add .",
    "git-commit": "cz",
    "git-push": "git push",
    "git": "pnpm run git-add && pnpm run git-commit && pnpm run git-push",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "version": "pnpm run changelog",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "extract-i18n": "nx extract-i18n --output-path apps/app-name/src/i18n",
    "dev:ru": "nx serve --configuration=ru",
    "xliffmerge": "nx run app-name:xliffmerge",
    "dev:ssr": "nx run app-name:serve-ssr",
    "serve:ssr": "node dist/apps/app-name/server/main.js",
    "build:ssr": "nx build --prod && nx run app-name:server:production",
    "prerender": "nx run app-name:prerender",
    "dev:hmr": "nx serve --hmr"
  },
  "husky": {
    "hooks": {
      "pre-commit": "nx lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "private": true,
  "devDependencies": {
    "@angular-builders/custom-webpack": "11.0.0",
    "@angular-devkit/build-angular": "~0.1102.0",
    "@angular-eslint/eslint-plugin": "1.2.0",
    "@angular-eslint/eslint-plugin-template": "1.2.0",
    "@angular-eslint/template-parser": "1.2.0",
    "@angular/compiler-cli": "^11.2.0",
    "@angular/language-service": "^11.2.0",
    "@angular/localize": "^11.2.2",
    "@commitlint/cli": "12.0.0",
    "@commitlint/config-conventional": "12.0.0",
    "@nguniversal/builders": "^11.2.0",
    "@ngx-i18nsupport/tooling": "^8.0.3",
    "@nrwl/angular": "^11.3.1",
    "@nrwl/cli": "11.3.1",
    "@nrwl/cypress": "11.3.1",
    "@nrwl/eslint-plugin-nx": "11.3.1",
    "@nrwl/jest": "11.3.1",
    "@nrwl/linter": "11.3.1",
    "@nrwl/tao": "11.3.1",
    "@nrwl/workspace": "11.3.1",
    "@types/express": "^4.17.0",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.31",
    "@typescript-eslint/eslint-plugin": "4.15.2",
    "@typescript-eslint/parser": "4.15.2",
    "commitizen": "^4.2.3",
    "conventional-changelog-cli": "^2.1.1",
    "cypress": "^6.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "cz-format-extension": "^1.4.0",
    "dotenv": "8.2.0",
    "eslint": "7.20.0",
    "eslint-config-prettier": "8.0.0",
    "eslint-plugin-cypress": "^2.10.3",
    "husky": "5.1.1",
    "jest": "26.6.3",
    "jest-preset-angular": "8.3.2",
    "npm-check": "^5.9.2",
    "postcss-ng-tailwind-in-components": "0.0.3",
    "prettier": "2.2.1",
    "tailwindcss": "2.0.3",
    "ts-jest": "26.5.2",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.0",
    "typescript": "4.1.5"
  },
  "dependencies": {
    "@angular/animations": "^11.2.0",
    "@angular/cdk": "^11.2.1",
    "@angular/common": "^11.2.0",
    "@angular/compiler": "^11.2.0",
    "@angular/core": "^11.2.0",
    "@angular/forms": "^11.2.0",
    "@angular/material": "^11.2.1",
    "@angular/platform-browser": "^11.2.0",
    "@angular/platform-browser-dynamic": "^11.2.0",
    "@angular/platform-server": "^11.2.0",
    "@angular/router": "^11.2.0",
    "@ngneat/tailwind": "^6.0.3",
    "@nguniversal/express-engine": "^11.2.0",
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/line-clamp": "^0.2.0",
    "@tailwindcss/typography": "^0.4.0",
    "express": "^4.15.2",
    "normalize.css": "^8.0.1",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "0.11.4"
  }
}
